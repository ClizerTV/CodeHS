public class Messages extends ConsoleProgram
{
    public void run()
    {
        TextMessage t1 = new TextMessage(
            "Bart", "Lisa", "Where are you?");
        TextMessage t2 = new TextMessage(
            "Lisa", "Bart", "I'm at school!");
            System.out.println(t1);
            System.out.println(t2);
    }
}
